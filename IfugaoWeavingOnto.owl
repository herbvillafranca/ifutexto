        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <owl:annotatedTarget xml:lang="en">...younger shamans wear usually a ​ha’pe​ ​blanket [while they perform their rites]</owl:annotatedTarget>
        <reference>Lambrecht 1958</reference>
    </owl:Axiom>

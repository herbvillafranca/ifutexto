    <!-- http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ginallit -->

    <owl:Class rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ginallit">
        <rdfs:subClassOf rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Skirt"/>
        <alternateTerm xml:lang="ifk">Binnalit</alternateTerm>
        <alternateTerm xml:lang="ifk">Intinlu</alternateTerm>
        <alternateTerm xml:lang="ifk">Pfinalet</alternateTerm>
        <longDescription xml:lang="en">An all-around Ifugao striped skirt. It features a series of parallel black (or dark blue) and white stripes, and alternating black and white takdog stitches.</longDescription>
        <preferredTerm xml:lang="ifk">Ginallit</preferredTerm>
        <rdfs:comment xml:lang="en">Presently, the takdog is sewn with alternating black and red threads instead of the more traditional black and white. The selvedges of the skirt are decorated by the kutilap stitch, triangular embroideries of red and black thread alternating every two inches. The skirt’s cut edges are hemmed then embroidered with the hambulud in red and yellow threads, again alternating in colors every 5 cm.</rdfs:comment>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ginallit"/>
        <owl:annotatedProperty rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#alternateTerm"/>

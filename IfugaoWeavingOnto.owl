    <!-- http://www.habingkordilyera.ph/ifugao-weaving-ontology#Allot -->

    <owl:Class rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Allot">
        <rdfs:subClassOf rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Belt"/>
        <longDescription xml:lang="en">a kind of belt used in Ifugao</longDescription>
        <rdfs:comment xml:lang="en">Seen in Martin 2018</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ambayung -->

    <owl:Class rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ambayung">
        <rdfs:subClassOf rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#HipBag"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#hasColor"/>
                <owl:hasValue rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#hasDesign"/>
                <owl:hasValue rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Hinulgi"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <alternateTerm xml:lang="ifk">Ambayong</alternateTerm>
        <longDescription xml:lang="en">A type of butong, smaller than a pinu&apos;hha; woven in black yarn and has narrower stripes. The hinulgi stripe is a dominant motif.</longDescription>
        <preferredTerm xml:lang="ifk">Ambayung</preferredTerm>
        <shortDescription xml:lang="en">Male Hip Bag</shortDescription>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Ambayung"/>
        <owl:annotatedProperty rdf:resource="http://www.habingkordilyera.ph/ifugao-weaving-ontology#longDescription"/>
        <owl:annotatedTarget xml:lang="en">A type of butong, smaller than a pinu&apos;hha; woven in black yarn and has narrower stripes. The hinulgi stripe is a dominant motif.</owl:annotatedTarget>
        <reference>Martin 2018</reference>
    </owl:Axiom>
    



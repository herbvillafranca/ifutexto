                    <rdf:Description rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Loincloth"/>
                    <rdf:Description rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#Skirt"/>
                    <rdf:Description rdf:about="http://www.habingkordilyera.ph/ifugao-weaving-ontology#UpperGarment"/>
                </owl:unionOf>
            </owl:Class>
        </owl:disjointWith>
    </owl:Class>
    


    <!-- http://www.habingkordilyera.ph/ifugao-weaving-ontology#Balko -->


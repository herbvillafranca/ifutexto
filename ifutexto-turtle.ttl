#################################################################
#    Annotation properties
#################################################################

###  http://www.habingifugao.ph/ifutexto#altName
ito:altName rdf:type owl:AnnotationProperty ;
            rdfs:domain owl:Thing .


###  http://www.habingifugao.ph/ifutexto#longDescription
ito:longDescription rdf:type owl:AnnotationProperty ;
                    rdfs:domain owl:Thing .


###  http://www.habingifugao.ph/ifutexto#name
ito:name rdf:type owl:AnnotationProperty ;
         rdfs:domain owl:Thing .


###  http://www.habingifugao.ph/ifutexto#reference
ito:reference rdf:type owl:AnnotationProperty .


###  http://www.habingifugao.ph/ifutexto#shortDescription
ito:shortDescription rdf:type owl:AnnotationProperty ;
                     rdfs:domain owl:Thing .


#################################################################
#    Object Properties
#################################################################

###  http://www.habingifugao.ph/ifutexto#hasColor
ito:hasColor rdf:type owl:ObjectProperty ;
             rdfs:domain ito:Textile ;
             rdfs:range ito:Color .


###  http://www.habingifugao.ph/ifutexto#hasDesign
ito:hasDesign rdf:type owl:ObjectProperty ;
              owl:inverseOf ito:isADesignSeenIn ;
              rdfs:domain ito:Textile ;
              rdfs:range ito:Design .


###  http://www.habingifugao.ph/ifutexto#hasRegion
ito:hasRegion rdf:type owl:ObjectProperty ;
              rdfs:domain ito:Textile ;
              rdfs:range ito:TextileRegion .


###  http://www.habingifugao.ph/ifutexto#hasSubdesign
ito:hasSubdesign rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain ito:Design ;
                 rdfs:range ito:Design .


###  http://www.habingifugao.ph/ifutexto#hasSubregion
ito:hasSubregion rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain ito:TextileRegion ;
                 rdfs:range ito:TextileRegion .


###  http://www.habingifugao.ph/ifutexto#hasTechnique
ito:hasTechnique rdf:type owl:ObjectProperty ;
                 rdfs:domain ito:Textile ;
                 rdfs:range ito:Technique .


###  http://www.habingifugao.ph/ifutexto#isADesignInTextilesUsedBy
ito:isADesignInTextilesUsedBy rdf:type owl:ObjectProperty ;
                              rdfs:domain ito:Design ;
                              rdfs:range ito:User .


###  http://www.habingifugao.ph/ifutexto#isADesignInTextilesUsedIn
ito:isADesignInTextilesUsedIn rdf:type owl:ObjectProperty ;
                              rdfs:domain ito:Design ;
                              rdfs:range ito:Usage .


###  http://www.habingifugao.ph/ifutexto#isADesignMadeWithTechnique
ito:isADesignMadeWithTechnique rdf:type owl:ObjectProperty ;
                               owl:inverseOf ito:isATechniqueThatFeaturesDesign ;
                               rdfs:domain ito:Design ;
                               rdfs:range ito:Technique .


###  http://www.habingifugao.ph/ifutexto#isADesignSeenIn
ito:isADesignSeenIn rdf:type owl:ObjectProperty ;
                    rdfs:domain ito:Design ;
                    rdfs:range ito:Textile .


###  http://www.habingifugao.ph/ifutexto#isADesignThatAppearsWithDesign
ito:isADesignThatAppearsWithDesign rdf:type owl:ObjectProperty ,
                                            owl:SymmetricProperty ,
                                            owl:TransitiveProperty ;

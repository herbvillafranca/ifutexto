   owl:annotatedProperty :alternateTerm ;
   owl:annotatedTarget "Intinlu"@ifk ;
   :reference "Martin 2018"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Ginallit ;
   owl:annotatedProperty :alternateTerm ;
   owl:annotatedTarget "Pfinalet"@ifk ;
   :reference "Martin 2018"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Ginallit ;

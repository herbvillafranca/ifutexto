ito:isUsedByCommunity rdf:type owl:ObjectProperty ;
                      rdfs:domain ito:Textile ;
                      rdfs:range ito:Community .


###  http://www.habingifugao.ph/ifutexto#isUsedDuring
ito:isUsedDuring rdf:type owl:ObjectProperty ;
                 rdfs:domain ito:Textile ;
                 rdfs:range ito:Usage .


###  http://www.habingifugao.ph/ifutexto#isUsedWithTextile
ito:isUsedWithTextile rdf:type owl:ObjectProperty ,
                               owl:SymmetricProperty ,
                               owl:TransitiveProperty ;
                      rdfs:domain ito:Textile ;
                      rdfs:range ito:Textile ;
                      ito:longDescription "When two textiles are used together/simultaneously"@en .


#################################################################
#    Classes
#################################################################

###  http://www.habingifugao.ph/ifutexto#AgeGroup
ito:AgeGroup rdf:type owl:Class ;
             rdfs:subClassOf ito:User ;
             ito:name "Age Group"@en .


###  http://www.habingifugao.ph/ifutexto#Allot
ito:Allot rdf:type owl:Class ;
          rdfs:subClassOf ito:Belt ;
          ito:longDescription "a kind of belt used in Ifugao"@en ;
          ito:name "Allot"@ifk ;
          rdfs:comment "Seen in Martin 2018"@en .


###  http://www.habingifugao.ph/ifutexto#Ambayung
ito:Ambayung rdf:type owl:Class ;
             rdfs:subClassOf ito:HipBag ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ito:hasColor ;
                               owl:hasValue ito:Black
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty ito:hasDesign ;
                               owl:hasValue ito:Hinulgi
                             ] ;
             ito:altName "Ambayong"@ifk ;
             ito:longDescription "A type of butong, smaller than a pinu'hha; woven in black yarn and has narrower stripes. The hinulgi stripe is a dominant motif."@en ;
             ito:name "Ambayung"@ifk ;
             ito:shortDescription "Male Hip Bag"@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ito:Ambayung ;
   owl:annotatedProperty ito:longDescription ;
   owl:annotatedTarget "A type of butong, smaller than a pinu'hha; woven in black yarn and has narrower stripes. The hinulgi stripe is a dominant motif."@en ;
   ito:reference "Martin 2018"
 ] .


###  http://www.habingifugao.ph/ifutexto#AnimalDesign
ito:AnimalDesign rdf:type owl:Class ;
                 rdfs:subClassOf ito:Design ;
                 ito:name "Animal"@en ;
                 rdfs:comment "a Design subclass that features animal designs, motifs, and patterns"@en .


###  http://www.habingifugao.ph/ifutexto#Bag
ito:Bag rdf:type owl:Class ;
        rdfs:subClassOf ito:Textile ;
        owl:disjointWith [ rdf:type owl:Class ;
                           owl:unionOf ( ito:Belt
                                         ito:Blanket
                                         ito:Headdress
                                         ito:Loincloth
                                         ito:Skirt
                                         ito:UpperGarment
                                       )
                         ] ;
        ito:name "Bag"@en .


###  http://www.habingifugao.ph/ifutexto#Balko
ito:Balko rdf:type owl:Class ;
          rdfs:subClassOf ito:Belt ;
          ito:longDescription "a generic term for a woman's belt"@en ;
          ito:name "Balko"@ifk .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ito:Balko ;
   owl:annotatedProperty ito:longDescription ;
   owl:annotatedTarget "a generic term for a woman's belt"@en ;
   ito:reference "Lambrecht 1958"
 ] .


###  http://www.habingifugao.ph/ifutexto#Bayyaung
ito:Bayyaung rdf:type owl:Class ;
             rdfs:subClassOf ito:Blanket ,
                             ito:Headdress ,
                             ito:Shroud ;
             ito:altName "Bayaong"@ifk ,
                         "Bayyaong"@ifk ,
                         "Chili"@ifk ;
             ito:longDescription "The bayaung has become the standard Ifugao blanket at present. Men of all ages regardless of social status wear this red and black blanket during social events requiring the use of traditional attires. Among earlier generations however, the bayaung was exclusive to elder members of the upper class and to ritualists who officiate the baki rituals."@en ;
             ito:name "Bayyaung"@ifk ;
             rdfs:comment "Chili​ also known as ​baya-ong​, a high-class shroud is used by the Lagawe folk accompanied with a ​hape​, a decorative attire placed across the body."@en ,
                          "When used as a funerary garment (gagaom), the bayaung serves as a pongot or a headdress and not as a blanket as the gamongs already serve this purpose."@en ,
                          "[Bayaong blankets] serve as an upper garment, occasionally as a turban, mostly for older people, on festival days such as marriage feasts."@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ito:Bayyaung ;
   owl:annotatedProperty ito:altName ;
   owl:annotatedTarget "Chili"@ifk ;
   rdfs:comment "Celino 1990"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ito:Bayyaung ;
   owl:annotatedProperty ito:longDescription ;
   owl:annotatedTarget "The bayaung has become the standard Ifugao blanket at present. Men of all ages regardless of social status wear this red and black blanket during social events requiring the use of traditional attires. Among earlier generations however, the bayaung was exclusive to elder members of the upper class and to ritualists who officiate the baki rituals."@en ;
   ito:reference "Martin 2018"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource ito:Bayyaung ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "[Bayaong blankets] serve as an upper garment, occasionally as a turban, mostly for older people, on festival days such as marriage feasts."@en ;
   ito:reference "Lambrecht 1958"
 ] .


